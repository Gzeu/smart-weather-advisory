    "type": "git",
    "url": "git+https://github.com/Gzeu/smart-weather-advisory.git"
  },
  "bugs": {
    "url": "https://github.com/Gzeu/smart-weather-advisory/issues"
  },
  "homepage": "https://github.com/Gzeu/smart-weather-advisory#readme",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
